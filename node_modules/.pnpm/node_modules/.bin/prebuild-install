#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/marcelo/Source/local-server/node_modules/.pnpm/prebuild-install@6.0.1/node_modules/prebuild-install/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/prebuild-install@6.0.1/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/node_modules:/Users/marcelo/Source/local-server/node_modules:/Users/marcelo/Source/node_modules:/Users/marcelo/node_modules:/Users/node_modules:/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/node_modules/prebuild-install/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/marcelo/Source/local-server/node_modules/.pnpm/prebuild-install@6.0.1/node_modules/prebuild-install/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/prebuild-install@6.0.1/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/node_modules:/Users/marcelo/Source/local-server/node_modules:/Users/marcelo/Source/node_modules:/Users/marcelo/node_modules:/Users/node_modules:/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/node_modules/prebuild-install/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prebuild-install/bin.js" "$@"
else
  exec node  "$basedir/../prebuild-install/bin.js" "$@"
fi
