#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/marcelo/Source/local-server/node_modules/.pnpm/@babel+parser@7.13.13/node_modules/@babel/parser/bin/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/@babel+parser@7.13.13/node_modules/@babel/parser/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/@babel+parser@7.13.13/node_modules/@babel/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/@babel+parser@7.13.13/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/node_modules:/Users/marcelo/Source/local-server/node_modules:/Users/marcelo/Source/node_modules:/Users/marcelo/node_modules:/Users/node_modules:/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/pkg@5.4.1/node_modules/pkg/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/pkg@5.4.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/marcelo/Source/local-server/node_modules/.pnpm/@babel+parser@7.13.13/node_modules/@babel/parser/bin/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/@babel+parser@7.13.13/node_modules/@babel/parser/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/@babel+parser@7.13.13/node_modules/@babel/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/@babel+parser@7.13.13/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/node_modules:/Users/marcelo/Source/local-server/node_modules:/Users/marcelo/Source/node_modules:/Users/marcelo/node_modules:/Users/node_modules:/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/pkg@5.4.1/node_modules/pkg/node_modules:/Users/marcelo/Source/local-server/node_modules/.pnpm/pkg@5.4.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@babel+parser@7.13.13/node_modules/@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../../../../../@babel+parser@7.13.13/node_modules/@babel/parser/bin/babel-parser.js" "$@"
fi
